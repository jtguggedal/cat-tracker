menuconfig OUTPUT_MODULE
	bool "Output module"
	select LED_CONTROL
	default y

if OUTPUT_MODULE

config OUTPUT_THREAD_STACK_SIZE
	int "Output module thread stack size"
	default 512

rsource "../led/Kconfig"

endif # OUTPUT_MODULE

module = OUTPUT_MODULE
module-str = Output module
source "subsys/logging/Kconfig.template.log_config"
